{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true,
        "arrowFunctions": true,
        "forOf": true,
        "modules": true,
        "generators": true
    },
    "ecmaVersion": 6
  },
  "plugins": [
  ],
  "extends": ["eslint:recommended"],
  "rules": {
    "no-console": 0
  }
}
